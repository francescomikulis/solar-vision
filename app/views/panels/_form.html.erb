<%= form_for(@panel) do |f| %>
  <% if @panel.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@panel.errors.count, "error") %> prohibited this panel from being saved:</h2>

      <ul>
      <% @panel.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="TextInputs" class="PanelsPage">
    <div class="field">
      <%= f.label :name %><br>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :power %><br>
      <%= f.text_field :power %>
    </div>
    <div class="field float_left">
      <%= f.label :tracking %><br>
      <%= f.check_box :tracking %>
    </div>
    <div class="actions" >
      <%= f.submit :class => "btn btn-info btn-lg" %>
    </div>
  </div>
<% end %>
